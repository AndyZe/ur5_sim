<?xml version="1.0"?>
<launch>

  <!-- compliance trajectory controller -->
  <rosparam file="$(find ur_gazebo)/controller/compliance_trajectory_controller_ur5.yaml" command="load"/>
  <node name="compliance_trajectory_controller_loader" pkg="controller_manager" type="controller_manager" args="load compliance_trajectory_controller" respawn="false" output="screen"/>

  <!-- compliance velocity controller -->
  <rosparam file="$(find ur_gazebo)/controller/compliance_velocity_controller_ur5.yaml" command="load"/>
  <node name="compliance_velocity_controller_loader" pkg="controller_manager" type="controller_manager" args="load compliance_velocity_controller" respawn="false" output="screen"/>

  <!-- joint_state_controller -->
  <rosparam file="$(find ur_gazebo)/controller/joint_state_controller.yaml" command="load"/>
  <node name="joint_state_controller_loader" pkg="controller_manager" type="controller_manager" args="load joint_state_controller" respawn="false" output="screen"/>

</launch>